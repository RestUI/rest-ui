{
  "name": "rest-ui",
  "version": "1.0.1",
  "description": "Develop faster Admin UIs with the power of React/Redux stack",
  "files": [
    "*.md",
    "docs/*.md",
    "lib",
    "src"
  ],
  "main": "lib/index",
  "scripts": {
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/babel ./src -d lib --ignore '*.spec.js'",
    "build:example": "cd example && yarn build",
    "start:example": "yarn build && yarn build:example && serve example",
    "start": "serve example",
    "test": "mocha './src/**/*.spec.js' --compilers js:babel-register --require babel-polyfill",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha -- ./src/**/*.spec.js"
  },
  "authors": "RestUI",
  "repository": "RestUI/rest-ui",
  "homepage": "https://github.com/RestUI/rest-ui#readme",
  "bugs": "https://github.com/RestUI/rest-ui/issues",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "~0.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "~6.8.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-0": "~6.16.0",
    "babel-register": "~6.18.0",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "css-loader": "~0.26.0",
    "enzyme": "~2.6.0",
    "eslint": "~3.11.0",
    "eslint-config-airbnb": "~13.0.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~2.2.3",
    "eslint-plugin-react": "~6.7.1",
    "extract-text-webpack-plugin": "~1.0.1",
    "full-icu": "~1.0.3",
    "ignore-styles": "~5.0.1",
    "isparta": "^4.0.0",
